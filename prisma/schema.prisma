  dateofbirth               String
  age                       Int
  educationlevel            String
  occupation                String
  religion                  String
  denomination              String
  photo                     String    @default("null")
  healthinstitution         String
  healthinstitutioninitials String
  clinicid                  Int       @default(0)
  patientcategory           String
  maritalstatus             String
  residentialaddress        String
  lga                       String
  town                      String
  postalcode                String
  currentstate              String
  nationality               String
  addressline2              String
  country                   String
  stateoforigin             String
  gender                    String
  mobile                    String
  email                     String    @unique
  nokfirstname              String
  noklastname               String
  nokrelationship           String
  nokphone                  String
  nokaddress                String
  hypertension              String
  burningsensation          String
  heartdisease              String
  swelling                  String
  sicklecellanaemia         String
  chroniclowerabdominalpain String
  diabetes                  String
  painfulurination          String
  tuberculosis              String
  genitalsores              String
  asthma                    String
  genitalgrowth             String
  epilepsy                  String
  mentaldisorder            String
  previoussurgery           String
  drughistory               String
  others                    String
  generalnotes              String
  postpartum                String
  hypertensionf             String
  sicklecellanaemiaf        String
  asthmaf                   String
  heartdiseasef             String
  diabetesf                 String
  twinf                     String
  birthdefects              String
  mentaldisorderfh          String
  othersfh                  String
  billsbalance              String[]
  totalamountbilled         Int
  totalamountpaid           Int
  balanceamount             Int
  recordentrydate           String
  registrationdate          DateTime  @default(now())
  updatedat                 DateTime? @updatedAt
}

