generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id                        String    @id @default(cuid())
  title                     String
  lastname                  String
  firstname                 String
